<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:macros="sap.fe.macros">

    
    <VBox
        id="PartnerFormRoot"
        width="100%"
        class="sapUiSmallMargin">

        <Title
            text="Partner Details"
            level="H4"
            class="sapUiSmallMarginBottom" />

        <form:SimpleForm
            editable="true"
            layout="ResponsiveGridLayout"
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="4"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="4"
            emptySpanL="4"
            emptySpanM="0"
            emptySpanS="0"
            columnsXL="1"
            columnsL="1"
            columnsM="1">

            <!-- Field 1 -->
            <Label text="Incoming Payment method" />
            <macros:Field value="{paymentMethod}" />

            <!-- Field 2 -->
            <Label text="Ar billing job" />
            <macros:Field value="{arBillingJob}" />

            <!-- Field 3 -->
            <Label text="Payoff Lock" />
            <macros:Field value="{payoffLock}" />

        </form:SimpleForm>
    </VBox>

</core:FragmentDefinition>
